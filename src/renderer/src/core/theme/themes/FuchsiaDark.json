{
  "name": "FuchsiaDark",
  "monaco": {
    "base": "vs-dark",
    "inherit": true,
    "rules": [
      {
        "foreground": "94a3b8",
        "token": "comment"
      },
      {
        "foreground": "d946ef",
        "token": "constant"
      },
      {
        "foreground": "e879f9",
        "token": "entity"
      },
      {
        "foreground": "c026d3",
        "token": "keyword"
      },
      {
        "foreground": "c026d3",
        "token": "storage"
      },
      {
        "foreground": "34d399",
        "token": "string"
      },
      {
        "foreground": "34d399",
        "token": "meta.verbatim"
      },
      {
        "foreground": "d946ef",
        "token": "support"
      },
      {
        "foreground": "f87171",
        "fontStyle": "italic",
        "token": "invalid.deprecated"
      },
      {
        "foreground": "fdf4ff",
        "background": "dc2626",
        "token": "invalid.illegal"
      },
      {
        "foreground": "e879f9",
        "fontStyle": "italic",
        "token": "entity.other.inherited-class"
      },
      {
        "foreground": "e879f9",
        "token": "string constant.other.placeholder"
      },
      {
        "foreground": "fdf4ff",
        "token": "meta.function-call.py"
      },
      {
        "foreground": "94a3b8",
        "token": "meta.tag"
      },
      {
        "foreground": "94a3b8",
        "token": "meta.tag entity"
      },
      {
        "foreground": "fdf4ff",
        "token": "entity.name.section"
      }
    ],
    "colors": {
      "editor.foreground": "#fdf4ff",
      "editor.background": "#1f0a2e",
      "editor.selectionBackground": "#701a75",
      "editor.lineHighlightBackground": "#2d0f4a",
      "editorCursor.foreground": "#d946ef",
      "editorWhitespace.foreground": "#d946ef"
    }
  },

{
  "name": "SkyDark",
  "monaco": {
    "base": "vs-dark",
    "inherit": true,
    "rules": [
      {
        "foreground": "94a3b8",
        "token": "comment"
      },
      {
        "foreground": "38bdf8",
        "token": "constant"
      },
      {
        "foreground": "7dd3fc",
        "token": "entity"
      },
      {
        "foreground": "0ea5e9",
        "token": "keyword"
      },
      {
        "foreground": "0ea5e9",
        "token": "storage"
      },
      {
        "foreground": "34d399",

      },
      {
        "foreground": "34d399",
        "token": "meta.verbatim"
      },
      {
        "foreground": "38bdf8",
        "token": "support"
      },
      {
        "foreground": "f87171",
        "fontStyle": "italic",
        "token": "invalid.deprecated"
      },
      {
        "foreground": "f0f9ff",
        "background": "dc2626",
        "token": "invalid.illegal"
      },
      {
        "foreground": "7dd3fc",
        "fontStyle": "italic",
        "token": "entity.other.inherited-class"
      },
      {
        "foreground": "7dd3fc",
        "token": "string constant.other.placeholder"
      },
      {
        "foreground": "f0f9ff",
        "token": "meta.function-call.py"
      },
      {
        "foreground": "94a3b8",
        "token": "meta.tag"
      },
      {
        "foreground": "94a3b8",
        "token": "meta.tag entity"
      },
      {
        "foreground": "f0f9ff",
        "token": "entity.name.section"
      }
    ],
    "colors": {
      "editor.foreground": "#f0f9ff",

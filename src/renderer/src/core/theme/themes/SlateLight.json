{
  "name": "SlateLight",
  "monaco": {
    "base": "vs",
    "inherit": true,
    "rules": [
      {
        "foreground": "64748b",
        "token": "comment"
      },
      {
        "foreground": "475569",
        "token": "constant"
      },
      {
        "foreground": "64748b",
        "token": "entity"
      },
      {
        "foreground": "334155",
        "token": "keyword"
      },
      {
        "foreground": "334155",
        "token": "storage"
      },
      {
        "foreground": "059669",
        "token": "string"
      },
      {
        "foreground": "059669",
        "token": "meta.verbatim"
      },
      {
        "foreground": "475569",
        "token": "support"
      },
      {
        "foreground": "dc2626",
        "fontStyle": "italic",
        "token": "invalid.deprecated"
      },
      {
        "foreground": "0f172a",
        "background": "ef4444",
        "token": "invalid.illegal"
      },
      {
        "foreground": "64748b",
        "fontStyle": "italic",
        "token": "entity.other.inherited-class"
      },
      {
        "foreground": "64748b",
        "token": "string constant.other.placeholder"
      },
      {
        "foreground": "0f172a",
        "token": "meta.function-call.py"
      },
      {
        "foreground": "64748b",
        "token": "meta.tag"
      },
      {
        "foreground": "64748b",
        "token": "meta.tag entity"
      },
      {
        "foreground": "0f172a",
        "token": "entity.name.section"
      }
    ],
    "colors": {
      "editor.foreground": "#0f172a",
      "editor.background": "#f8fafc",
      "editor.selectionBackground": "#e2e8f0",
      "editor.lineHighlightBackground": "#f1f5f9",
      "editorCursor.foreground": "#475569",
      "editorWhitespace.foreground": "#cbd5e1"
    }
  },
  "tailwind": {
    "primary": "71 85 105",
    "background": "248 250 252",
    "textPrimary": "15 23 42",
    "textSecondary": "71 85 105",
    "border": "226 232 240",
    "borderHover": "203 213 225",
    "borderFocus": "148 163 184",
    "cardBackground": "255 255 255",
    "inputBackground": "255 255 255",
    "inputBorderDefault": "226 232 240",
    "inputBorderHover": "203 213 225",
    "inputBorderFocus": "71 85 105",
    "dialogBackground": "255 255 255",
    "dropdownBackground": "255 255 255",
    "dropdownItemHover": "241 245 249",
    "dropdownBorder": "226 232 240",

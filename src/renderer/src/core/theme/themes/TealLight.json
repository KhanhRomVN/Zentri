{
  "name": "TealLight",
  "monaco": {
    "base": "vs",
    "inherit": true,
    "rules": [
      {
        "foreground": "64748b",
        "token": "comment"
      },
      {
        "foreground": "0d9488",
        "token": "constant"
      },
      {
        "foreground": "14b8a6",
        "token": "entity"
      },
      {
        "foreground": "0f766e",
        "token": "keyword"
      },
      {
        "foreground": "0f766e",
        "token": "storage"
      },
      {
        "foreground": "059669",
        "token": "string"
      },
      {
        "foreground": "059669",
        "token": "meta.verbatim"
      },
      {
        "foreground": "0d9488",
        "token": "support"
      },
      {
        "foreground": "dc2626",
        "fontStyle": "italic",
        "token": "invalid.deprecated"
      },
      {
        "foreground": "0f172a",
        "background": "ef4444",
        "token": "invalid.illegal"
      },
      {
        "foreground": "14b8a6",
        "fontStyle": "italic",
        "token": "entity.other.inherited-class"
      },
      {
        "foreground": "14b8a6",
        "token": "string constant.other.placeholder"
      },
      {
        "foreground": "0f172a",
        "token": "meta.function-call.py"
      },
      {
        "foreground": "64748b",
        "token": "meta.tag"
      },
      {
        "foreground": "64748b",
        "token": "meta.tag entity"
      },
      {
        "foreground": "0f172a",
        "token": "entity.name.section"
      }
    ],
    "colors": {
      "editor.foreground": "#0f172a",
      "editor.background": "#f0fdfa",
      "editor.selectionBackground": "#99f6e4",
      "editor.lineHighlightBackground": "#ccfbf1",
      "editorCursor.foreground": "#14b8a6",
      "editorWhitespace.foreground": "#5eead4"
    }
  },
  "tailwind": {

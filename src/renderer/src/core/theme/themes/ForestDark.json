{
  "name": "ForestDark",
  "monaco": {
    "base": "vs-dark",
    "inherit": true,
    "rules": [
      {
        "foreground": "94a3b8",
        "token": "comment"
      },
      {
        "foreground": "4ade80",
        "token": "constant"
      },
      {
        "foreground": "86efac",
        "token": "entity"
      },
      {
        "foreground": "22c55e",
        "token": "keyword"
      },
      {
        "foreground": "22c55e",
        "token": "storage"
      },
      {
        "foreground": "34d399",
        "token": "string"
      },
      {
        "foreground": "34d399",
        "token": "meta.verbatim"
      },
      {
        "foreground": "4ade80",
        "token": "support"
      },
      {
        "foreground": "f87171",
        "fontStyle": "italic",
        "token": "invalid.deprecated"
      },
      {
        "foreground": "f0fdf4",
        "background": "dc2626",
        "token": "invalid.illegal"
      },
      {
        "foreground": "86efac",
        "fontStyle": "italic",
        "token": "entity.other.inherited-class"
      },
      {
        "foreground": "86efac",
        "token": "string constant.other.placeholder"
      },
      {
        "foreground": "f0fdf4",
        "token": "meta.function-call.py"
      },
      {
        "foreground": "94a3b8",
        "token": "meta.tag"
      },
      {
        "foreground": "94a3b8",
        "token": "meta.tag entity"
      },
      {
        "foreground": "f0fdf4",
        "token": "entity.name.section"
      }
    ],
    "colors": {
      "editor.foreground": "#f0fdf4",
      "editor.background": "#0a1f0f",
      "editor.selectionBackground": "#14532d",
      "editor.lineHighlightBackground": "#0f2d1a",
      "editorCursor.foreground": "#22c55e",
      "editorWhitespace.foreground": "#4ade80"
    }
  },

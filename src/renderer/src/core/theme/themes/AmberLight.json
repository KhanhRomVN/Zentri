{
  "name": "AmberLight",
  "monaco": {
    "base": "vs",
    "inherit": true,
    "rules": [
      {
        "foreground": "64748b",
        "token": "comment"
      },
      {
        "foreground": "d97706",
        "token": "constant"
      },
      {
        "foreground": "f59e0b",
        "token": "entity"
      },
      {
        "foreground": "b45309",
        "token": "keyword"
      },
      {
        "foreground": "b45309",
        "token": "storage"
      },
      {
        "foreground": "059669",
        "token": "string"
      },
      {
        "foreground": "059669",
        "token": "meta.verbatim"
      },
      {
        "foreground": "d97706",
        "token": "support"
      },
      {
        "foreground": "dc2626",
        "fontStyle": "italic",
        "token": "invalid.deprecated"
      },
      {
        "foreground": "0f172a",
        "background": "ef4444",
        "token": "invalid.illegal"
      },
      {
        "foreground": "f59e0b",
        "fontStyle": "italic",
        "token": "entity.other.inherited-class"
      },
      {
        "foreground": "f59e0b",
        "token": "string constant.other.placeholder"
      },
      {
        "foreground": "0f172a",
        "token": "meta.function-call.py"
      },
      {
        "foreground": "64748b",
        "token": "meta.tag"
      },
      {
        "foreground": "64748b",
        "token": "meta.tag entity"
      },
      {
        "foreground": "0f172a",
        "token": "entity.name.section"
      }
    ],
    "colors": {
      "editor.foreground": "#0f172a",
      "editor.background": "#fffbeb",
      "editor.selectionBackground": "#fde68a",
      "editor.lineHighlightBackground": "#fef3c7",
      "editorCursor.foreground": "#f59e0b",
      "editorWhitespace.foreground": "#fcd34d"
    }
  },
  "tailwind": {
    "primary": "245 158 11",
    "background": "255 251 235",
    "textPrimary": "15 23 42",
    "textSecondary": "71 85 105",
    "border": "253 230 138",

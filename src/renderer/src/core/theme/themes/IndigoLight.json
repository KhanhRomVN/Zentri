{
  "name": "IndigoLight",
  "monaco": {
    "base": "vs",
    "inherit": true,
    "rules": [
      {
        "foreground": "4338ca",
        "token": "comment"
      },
      {
        "foreground": "3730a3",
        "token": "constant"
      },
      {
        "foreground": "4f46e5",
        "token": "entity"
      },
      {
        "foreground": "4338ca",
        "token": "keyword"
      },
      {
        "foreground": "4338ca",
        "token": "storage"
      },
      {
        "foreground": "059669",
        "token": "string"
      },
      {
        "foreground": "059669",
        "token": "meta.verbatim"
      },
      {
        "foreground": "6366f1",
        "token": "support"
      },
      {
        "foreground": "dc2626",
        "fontStyle": "italic",
        "token": "invalid.deprecated"
      },
      {
        "foreground": "3730a3",
        "background": "ef4444",
        "token": "invalid.illegal"
      },
      {
        "foreground": "4f46e5",
        "fontStyle": "italic",
        "token": "entity.other.inherited-class"
      },
      {
        "foreground": "4f46e5",
        "token": "string constant.other.placeholder"
      },
      {
        "foreground": "3730a3",
        "token": "meta.function-call.py"
      },
      {
        "foreground": "4338ca",
        "token": "meta.tag"
      },
      {
        "foreground": "4338ca",
        "token": "meta.tag entity"
      },
      {
        "foreground": "3730a3",
        "token": "entity.name.section"
      }
    ],
    "colors": {
      "editor.foreground": "#3730a3",
      "editor.background": "#ffffff",
      "editor.selectionBackground": "#c7d2fe",
      "editor.lineHighlightBackground": "#e0e7ff",
      "editorCursor.foreground": "#4f46e5",
      "editorWhitespace.foreground": "#a5b4fc"
    }
  },

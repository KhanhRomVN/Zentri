{
  "name": "RoseLight",
  "monaco": {
    "base": "vs",
    "inherit": true,
    "rules": [
      {
        "foreground": "64748b",
        "token": "comment"
      },
      {
        "foreground": "e11d48",
        "token": "constant"
      },
      {
        "foreground": "fb7185",
        "token": "entity"
      },
      {
        "foreground": "be123c",
        "token": "keyword"
      },
      {
        "foreground": "be123c",
        "token": "storage"
      },
      {
        "foreground": "059669",
        "token": "string"
      },
      {
        "foreground": "059669",
        "token": "meta.verbatim"
      },
      {
        "foreground": "e11d48",
        "token": "support"
      },
      {
        "foreground": "dc2626",
        "fontStyle": "italic",
        "token": "invalid.deprecated"
      },
      {
        "foreground": "0f172a",
        "background": "ef4444",
        "token": "invalid.illegal"
      },
      {
        "foreground": "fb7185",
        "fontStyle": "italic",
        "token": "entity.other.inherited-class"
      },
      {
        "foreground": "fb7185",
        "token": "string constant.other.placeholder"
      },
      {
        "foreground": "0f172a",
        "token": "meta.function-call.py"
      },
      {
        "foreground": "64748b",
        "token": "meta.tag"
      },
      {
        "foreground": "64748b",
        "token": "meta.tag entity"
      },
      {
        "foreground": "0f172a",
        "token": "entity.name.section"
      }
    ],
    "colors": {
      "editor.foreground": "#0f172a",
      "editor.background": "#fff1f2",
      "editor.selectionBackground": "#fecdd3",
      "editor.lineHighlightBackground": "#ffe4e6",
      "editorCursor.foreground": "#fb7185",
      "editorWhitespace.foreground": "#fda4af"
    }
  },
  "tailwind": {
    "primary": "251 113 133",
    "background": "255 241 242",
    "textPrimary": "15 23 42",
    "textSecondary": "71 85 105",
    "border": "254 205 211",

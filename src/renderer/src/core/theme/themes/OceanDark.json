{
  "name": "OceanDark",
  "monaco": {
    "base": "vs-dark",
    "inherit": true,
    "rules": [
      {
        "foreground": "94a3b8",
        "token": "comment"
      },
      {
        "foreground": "22d3ee",
        "token": "constant"
      },
      {
        "foreground": "67e8f9",
        "token": "entity"
      },
      {
        "foreground": "06b6d4",
        "token": "keyword"
      },
      {
        "foreground": "06b6d4",
        "token": "storage"
      },
      {
        "foreground": "34d399",
        "token": "string"
      },
      {
        "foreground": "34d399",
        "token": "meta.verbatim"
      },
      {
        "foreground": "22d3ee",
        "token": "support"
      },
      {
        "foreground": "f87171",
        "fontStyle": "italic",
        "token": "invalid.deprecated"
      },
      {
        "foreground": "f0fdfa",
        "background": "dc2626",
        "token": "invalid.illegal"
      },
      {
        "foreground": "67e8f9",
        "fontStyle": "italic",
        "token": "entity.other.inherited-class"
      },
      {
        "foreground": "67e8f9",
        "token": "string constant.other.placeholder"
      },
      {
        "foreground": "f0fdfa",
        "token": "meta.function-call.py"
      },
      {
        "foreground": "94a3b8",
        "token": "meta.tag"
      },
      {
        "foreground": "94a3b8",
        "token": "meta.tag entity"
      },
      {
        "foreground": "f0fdfa",
        "token": "entity.name.section"
      }
    ],
    "colors": {
      "editor.foreground": "#ecfeff",
      "editor.background": "#0a1f1f",
      "editor.selectionBackground": "#164e63",
      "editor.lineHighlightBackground": "#0f2d2d",
      "editorCursor.foreground": "#06b6d4",
      "editorWhitespace.foreground": "#22d3ee40"
    }
  },
  "tailwind": {
    "primary": "#06b6d4",
    "background": "#0a1f1f",
    "textPrimary": "#ecfeff",
    "textSecondary": "#94a3b8",
    "border": "#164e63",
    "borderHover": "#0891b2",
    "borderFocus": "#06b6d4",
    "cardBackground": "#0f2d2d",
    "inputBackground": "#0a1f1f",
    "inputBorderDefault": "#164e63",
    "inputBorderHover": "#0891b2",
    "inputBorderFocus": "#06b6d4",
    "dialogBackground": "#0f2d2d",
    "dropdownBackground": "#0f2d2d",
    "dropdownItemHover": "#164e63",
    "dropdownBorder": "#164e63",
    "dropdownBorderHover": "#0891b2",
    "sidebarBackground": "#051a1a",
    "sidebarItemHover": "#0f2d2d",
    "sidebarItemFocus": "#164e63",
    "buttonBg": "#06b6d4",
    "buttonBgHover": "#0891b2",
    "buttonText": "#ffffff",
    "buttonBorder": "#0891b2",
    "buttonBorderHover": "#0e7490",
    "buttonSecondBg": "#164e63",
    "buttonSecondBgHover": "#0e7490",
    "bookmarkItemBg": "#164e63",
    "bookmarkItemText": "#ecfeff",
    "drawerBackground": "#0f2d2d",
    "clockGradientFrom": "#06b6d4",
    "clockGradientTo": "#0891b2",
    "cardShadow": "0 1px 3px 0 rgba(6, 182, 212, 0.3)",
    "dialogShadow": "0 25px 50px -12px rgba(6, 182, 212, 0.5)",
    "dropdownShadow": "0 10px 15px -3px rgba(6, 182, 212, 0.3)",
    "tableHeaderBg": "#051a1a",
    "tableHoverHeaderBg": "#051a1a",
    "tableBodyBg": "#0f2d2d",
    "tableHoverItemBodyBg": "#164e63",
    "tableFocusItemBodyBg": "#155e75",
    "tableFooterBg": "#051a1a",
    "tableHoverFooterBg": "#051a1a",
    "tableBorder": "#164e63",
    "tabBackground": "#0f2d2d",
    "tabBorder": "#164e63",
    "tabHoverBorder": "#0891b2",
    "tabItemBackground": "transparent",
    "tabItemHoverBg": "#164e63",
    "tabItemFocusBg": "#155e75",
    "tabItemBorder": "transparent",
    "tabItemHoverBorder": "#0891b2",
    "tabItemFocusBorder": "#06b6d4",
    "divider": "#353535"
  }
}

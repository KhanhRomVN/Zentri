{
  "name": "SunsetLight",
  "monaco": {
    "base": "vs",
    "inherit": true,
    "rules": [
      {
        "foreground": "64748b",
        "token": "comment"
      },
      {
        "foreground": "ea580c",
        "token": "constant"
      },
      {
        "foreground": "f97316",
        "token": "entity"
      },
      {
        "foreground": "dc2626",
        "token": "keyword"
      },
      {
        "foreground": "dc2626",
        "token": "storage"
      },
      {
        "foreground": "059669",
        "token": "string"
      },
      {
        "foreground": "059669",
        "token": "meta.verbatim"
      },
      {
        "foreground": "ea580c",
        "token": "support"
      },
      {
        "foreground": "dc2626",
        "fontStyle": "italic",
        "token": "invalid.deprecated"
      },
      {
        "foreground": "0f172a",
        "background": "ef4444",
        "token": "invalid.illegal"
      },
      {
        "foreground": "f97316",
        "fontStyle": "italic",
        "token": "entity.other.inherited-class"
      },
      {
        "foreground": "f97316",
        "token": "string constant.other.placeholder"
      },
      {
        "foreground": "0f172a",
        "token": "meta.function-call.py"
      },
      {
        "foreground": "64748b",
        "token": "meta.tag"
      },
      {
        "foreground": "64748b",
        "token": "meta.tag entity"
      },
      {
        "foreground": "0f172a",
        "token": "entity.name.section"
      }
    ],
    "colors": {
      "editor.foreground": "#0f172a",
      "editor.background": "#fff7ed",
      "editor.selectionBackground": "#fed7aa",
      "editor.lineHighlightBackground": "#ffedd5",
      "editorCursor.foreground": "#f97316",
      "editorWhitespace.foreground": "#fdba74"
    }
  },
  "tailwind": {
    "primary": "249 115 22",
    "background": "255 247 237",
    "textPrimary": "15 23 42",
    "textSecondary": "71 85 105",
    "border": "254 215 170",

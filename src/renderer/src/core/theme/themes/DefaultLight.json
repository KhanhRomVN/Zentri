{
  "name": "DefaultLight",
  "monaco": {
    "base": "vs",
    "inherit": true,
    "rules": [
      {
        "foreground": "475569",
        "token": "comment"
      },
      {
        "foreground": "1d4ed8",
        "token": "constant"
      },
      {
        "foreground": "3686ff",
        "token": "entity"
      },
      {
        "foreground": "2563eb",
        "token": "keyword"
      },
      {
        "foreground": "2563eb",
        "token": "storage"
      },
      {
        "foreground": "059669",
        "token": "string"
      },
      {
        "foreground": "059669",
        "token": "meta.verbatim"
      },
      {
        "foreground": "1d4ed8",
        "token": "support"
      },
      {
        "foreground": "dc2626",
        "fontStyle": "italic",
        "token": "invalid.deprecated"
      },
      {
        "foreground": "0f172a",
        "background": "ef4444",
        "token": "invalid.illegal"
      },
      {
        "foreground": "3686ff",
        "fontStyle": "italic",
        "token": "entity.other.inherited-class"
      },
      {
        "foreground": "3686ff",
        "token": "string constant.other.placeholder"
      },
      {
        "foreground": "0f172a",
        "token": "meta.function-call.py"
      },
      {
        "foreground": "475569",
        "token": "meta.tag"
      },
      {
        "foreground": "475569",
        "token": "meta.tag entity"
      },
      {
        "foreground": "0f172a",
        "token": "entity.name.section"
      }
    ],
    "colors": {
      "editor.foreground": "#0f172a",
      "editor.background": "#ffffff",
      "editor.selectionBackground": "#e2e8f0",
      "editor.lineHighlightBackground": "#f8fafc",
      "editorCursor.foreground": "#3686ff",
      "editorWhitespace.foreground": "#cbd5e1"
    }
  },
  "tailwind": {

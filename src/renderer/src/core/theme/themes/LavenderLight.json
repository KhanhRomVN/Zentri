{
  "name": "LavenderLight",
  "monaco": {
    "base": "vs",
    "inherit": true,
    "rules": [
      {
        "foreground": "64748b",
        "token": "comment"
      },
      {
        "foreground": "7c3aed",
        "token": "constant"
      },
      {
        "foreground": "a78bfa",
        "token": "entity"
      },
      {
        "foreground": "6d28d9",
        "token": "keyword"
      },
      {
        "foreground": "6d28d9",
        "token": "storage"
      },
      {
        "foreground": "059669",
        "token": "string"
      },
      {
        "foreground": "059669",
        "token": "meta.verbatim"
      },
      {
        "foreground": "7c3aed",
        "token": "support"
      },
      {
        "foreground": "dc2626",
        "fontStyle": "italic",
        "token": "invalid.deprecated"
      },
      {
        "foreground": "0f172a",
        "background": "ef4444",
        "token": "invalid.illegal"
      },
      {
        "foreground": "a78bfa",
        "fontStyle": "italic",
        "token": "entity.other.inherited-class"
      },
      {
        "foreground": "a78bfa",
        "token": "string constant.other.placeholder"
      },
      {
        "foreground": "0f172a",
        "token": "meta.function-call.py"
      },
      {
        "foreground": "64748b",
        "token": "meta.tag"
      },
      {
        "foreground": "64748b",
        "token": "meta.tag entity"
      },
      {
        "foreground": "0f172a",
        "token": "entity.name.section"
      }
    ],
    "colors": {
      "editor.foreground": "#0f172a",
      "editor.background": "#faf5ff",
      "editor.selectionBackground": "#e9d5ff",
      "editor.lineHighlightBackground": "#f3e8ff",
      "editorCursor.foreground": "#a78bfa",
      "editorWhitespace.foreground": "#c4b5fd"
    }
  },

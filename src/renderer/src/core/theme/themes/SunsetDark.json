{
  "name": "SunsetDark",
  "monaco": {
    "base": "vs-dark",
    "inherit": true,
    "rules": [
      {
        "foreground": "94a3b8",
        "token": "comment"
      },
      {
        "foreground": "fb923c",
        "token": "constant"
      },
      {
        "foreground": "fdba74",
        "token": "entity"
      },
      {
        "foreground": "f97316",
        "token": "keyword"
      },
      {
        "foreground": "f97316",
        "token": "storage"
      },
      {
        "foreground": "34d399",
        "token": "string"
      },
      {
        "foreground": "34d399",
        "token": "meta.verbatim"
      },
      {
        "foreground": "fb923c",
        "token": "support"
      },
      {
        "foreground": "f87171",
        "fontStyle": "italic",
        "token": "invalid.deprecated"
      },
      {
        "foreground": "fff7ed",
        "background": "dc2626",
        "token": "invalid.illegal"
      },
      {
        "foreground": "fdba74",
        "fontStyle": "italic",
        "token": "entity.other.inherited-class"
      },
      {
        "foreground": "fdba74",
        "token": "string constant.other.placeholder"
      },
      {
        "foreground": "fff7ed",
        "token": "meta.function-call.py"
      },
      {
        "foreground": "94a3b8",
        "token": "meta.tag"
      },
      {
        "foreground": "94a3b8",
        "token": "meta.tag entity"
      },
      {
        "foreground": "fff7ed",
        "token": "entity.name.section"
      }
    ],
    "colors": {
      "editor.foreground": "#fff7ed",
      "editor.background": "#1a0f0a",
      "editor.selectionBackground": "#431407",
      "editor.lineHighlightBackground": "#2d1a0f",
      "editorCursor.foreground": "#f97316",
      "editorWhitespace.foreground": "#fb923c"
    }
  },

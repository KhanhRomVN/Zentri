{
  "name": "FuchsiaLight",
  "monaco": {
    "base": "vs",
    "inherit": true,
    "rules": [
      {
        "foreground": "64748b",
        "token": "comment"
      },
      {
        "foreground": "c026d3",
        "token": "constant"
      },
      {
        "foreground": "d946ef",
        "token": "entity"
      },
      {
        "foreground": "a21caf",
        "token": "keyword"
      },
      {
        "foreground": "a21caf",
        "token": "storage"
      },
      {
        "foreground": "059669",
        "token": "string"
      },
      {
        "foreground": "059669",
        "token": "meta.verbatim"
      },
      {
        "foreground": "c026d3",
        "token": "support"
      },
      {
        "foreground": "dc2626",
        "fontStyle": "italic",
        "token": "invalid.deprecated"
      },
      {
        "foreground": "0f172a",
        "background": "ef4444",
        "token": "invalid.illegal"
      },
      {
        "foreground": "d946ef",
        "fontStyle": "italic",
        "token": "entity.other.inherited-class"
      },
      {
        "foreground": "d946ef",
        "token": "string constant.other.placeholder"
      },
      {
        "foreground": "0f172a",
        "token": "meta.function-call.py"
      },
      {
        "foreground": "64748b",
        "token": "meta.tag"
      },
      {
        "foreground": "64748b",
        "token": "meta.tag entity"
      },
      {
        "foreground": "0f172a",
        "token": "entity.name.section"
      }
    ],
    "colors": {
      "editor.foreground": "#0f172a",
      "editor.background": "#fdf4ff",
      "editor.selectionBackground": "#f5d0fe",
      "editor.lineHighlightBackground": "#fae8ff",
      "editorCursor.foreground": "#d946ef",
      "editorWhitespace.foreground": "#e879f9"
    }
  },
  "tailwind": {

{
  "name": "FuchsiaLight",
  "monaco": {
    "base": "vs",
    "inherit": true,
    "rules": [
      {
        "foreground": "64748b",
        "token": "comment"
      },
      {
        "foreground": "c026d3",
        "token": "constant"
      },
      {
        "foreground": "d946ef",
        "token": "entity"
      },
      {
        "foreground": "a21caf",
        "token": "keyword"
      },
      {
        "foreground": "a21caf",
        "token": "storage"
      },
      {
        "foreground": "059669",
        "token": "string"
      },
      {
        "foreground": "059669",
        "token": "meta.verbatim"
      },
      {
        "foreground": "c026d3",
        "token": "support"
      },
      {
        "foreground": "dc2626",
        "fontStyle": "italic",
        "token": "invalid.deprecated"
      },
      {
        "foreground": "0f172a",
        "background": "ef4444",
        "token": "invalid.illegal"
      },
      {
        "foreground": "d946ef",
        "fontStyle": "italic",
        "token": "entity.other.inherited-class"
      },
      {
        "foreground": "d946ef",
        "token": "string constant.other.placeholder"
      },
      {
        "foreground": "0f172a",
        "token": "meta.function-call.py"
      },
      {
        "foreground": "64748b",
        "token": "meta.tag"
      },
      {
        "foreground": "64748b",
        "token": "meta.tag entity"
      },
      {
        "foreground": "0f172a",
        "token": "entity.name.section"
      }
    ],
    "colors": {
      "editor.foreground": "#0f172a",
      "editor.background": "#fdf4ff",
      "editor.selectionBackground": "#f5d0fe",
      "editor.lineHighlightBackground": "#fae8ff",
      "editorCursor.foreground": "#d946ef",
      "editorWhitespace.foreground": "#e879f940"
    }
  },
  "tailwind": {
    "primary": "#d946ef",
    "background": "#fdf4ff",
    "textPrimary": "#0f172a",
    "textSecondary": "#475569",
    "border": "#f5d0fe",
    "borderHover": "#e879f9",
    "borderFocus": "#d946ef",
    "cardBackground": "#ffffff",
    "inputBackground": "#ffffff",
    "inputBorderDefault": "#f5d0fe",
    "inputBorderHover": "#e879f9",
    "inputBorderFocus": "#d946ef",
    "dialogBackground": "#ffffff",
    "dropdownBackground": "#ffffff",
    "dropdownItemHover": "#fae8ff",
    "dropdownBorder": "#f5d0fe",
    "dropdownBorderHover": "#e879f9",
    "sidebarBackground": "#fae8ff",
    "sidebarItemHover": "#f5d0fe",
    "sidebarItemFocus": "#e879f9",
    "buttonBg": "#d946ef",
    "buttonBgHover": "#c026d3",
    "buttonText": "#ffffff",
    "buttonBorder": "#c026d3",
    "buttonBorderHover": "#a21caf",
    "buttonSecondBg": "#f5d0fe",
    "buttonSecondBgHover": "#e879f9",
    "bookmarkItemBg": "#f5d0fe",
    "bookmarkItemText": "#0f172a",
    "drawerBackground": "#ffffff",
    "clockGradientFrom": "#d946ef",
    "clockGradientTo": "#c026d3",
    "cardShadow": "0 1px 3px 0 rgb(217 70 239 / 0.1)",
    "dialogShadow": "0 25px 50px -12px rgb(217 70 239 / 0.25)",
    "dropdownShadow": "0 10px 15px -3px rgb(217 70 239 / 0.1)",
    "tableHeaderBg": "#fae8ff",
    "tableHoverHeaderBg": "#fae8ff",
    "tableBodyBg": "#ffffff",
    "tableHoverItemBodyBg": "#f5d0fe",
    "tableFocusItemBodyBg": "#e879f9",
    "tableFooterBg": "#fae8ff",
    "tableHoverFooterBg": "#fae8ff",
    "tableBorder": "#f5d0fe",
    "tabBackground": "#ffffff",
    "tabBorder": "#f5d0fe",
    "tabHoverBorder": "#e879f9",
    "tabItemBackground": "transparent",
    "tabItemHoverBg": "#fae8ff",
    "tabItemFocusBg": "#f5d0fe",
    "tabItemBorder": "transparent",
    "tabItemHoverBorder": "#e879f9",
    "tabItemFocusBorder": "#d946ef",
    "divider": "#e5e7eb"
  }
}

{
  "name": "LavenderDark",
  "monaco": {
    "base": "vs-dark",
    "inherit": true,
    "rules": [
      {
        "foreground": "94a3b8",
        "token": "comment"
      },
      {
        "foreground": "a78bfa",
        "token": "constant"
      },
      {
        "foreground": "c4b5fd",
        "token": "entity"
      },
      {
        "foreground": "8b5cf6",
        "token": "keyword"
      },
      {
        "foreground": "8b5cf6",
        "token": "storage"
      },
      {
        "foreground": "34d399",
        "token": "string"
      },
      {
        "foreground": "34d399",
        "token": "meta.verbatim"
      },
      {
        "foreground": "a78bfa",
        "token": "support"
      },
      {
        "foreground": "f87171",
        "fontStyle": "italic",
        "token": "invalid.deprecated"
      },
      {
        "foreground": "faf5ff",
        "background": "dc2626",
        "token": "invalid.illegal"
      },
      {
        "foreground": "c4b5fd",
        "fontStyle": "italic",
        "token": "entity.other.inherited-class"
      },
      {
        "foreground": "c4b5fd",
        "token": "string constant.other.placeholder"
      },
      {
        "foreground": "faf5ff",
        "token": "meta.function-call.py"
      },
      {
        "foreground": "94a3b8",
        "token": "meta.tag"
      },
      {
        "foreground": "94a3b8",
        "token": "meta.tag entity"
      },
      {
        "foreground": "faf5ff",
        "token": "entity.name.section"
      }
    ],
    "colors": {
      "editor.foreground": "#faf5ff",
      "editor.background": "#1a0f2e",
      "editor.selectionBackground": "#3b0764",
      "editor.lineHighlightBackground": "#2d1a4a",
      "editorCursor.foreground": "#a78bfa",
      "editorWhitespace.foreground": "#a78bfa"
    }
  },

{
  "name": "SlateDark",
  "monaco": {
    "base": "vs-dark",
    "inherit": true,
    "rules": [
      {
        "foreground": "94a3b8",
        "token": "comment"
      },
      {
        "foreground": "cbd5e1",
        "token": "constant"
      },
      {
        "foreground": "e2e8f0",
        "token": "entity"
      },
      {
        "foreground": "94a3b8",
        "token": "keyword"
      },
      {
        "foreground": "94a3b8",
        "token": "storage"
      },
      {
        "foreground": "34d399",
        "token": "string"
      },
      {
        "foreground": "34d399",
        "token": "meta.verbatim"
      },
      {
        "foreground": "cbd5e1",
        "token": "support"
      },
      {
        "foreground": "f87171",
        "fontStyle": "italic",
        "token": "invalid.deprecated"
      },
      {
        "foreground": "f8fafc",
        "background": "dc2626",
        "token": "invalid.illegal"
      },
      {
        "foreground": "e2e8f0",
        "fontStyle": "italic",
        "token": "entity.other.inherited-class"
      },
      {
        "foreground": "e2e8f0",
        "token": "string constant.other.placeholder"
      },
      {
        "foreground": "f8fafc",
        "token": "meta.function-call.py"
      },
      {
        "foreground": "94a3b8",
        "token": "meta.tag"
      },
      {
        "foreground": "94a3b8",
        "token": "meta.tag entity"
      },
      {
        "foreground": "f8fafc",
        "token": "entity.name.section"
      }
    ],
    "colors": {
      "editor.foreground": "#f8fafc",
      "editor.background": "#0f172a",
      "editor.selectionBackground": "#1e293b",
      "editor.lineHighlightBackground": "#1e293b",
      "editorCursor.foreground": "#94a3b8",
      "editorWhitespace.foreground": "#94a3b8"
    }
  },

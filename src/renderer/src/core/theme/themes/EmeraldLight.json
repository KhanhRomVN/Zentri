{
  "name": "EmeraldLight",
  "monaco": {
    "base": "vs",
    "inherit": true,
    "rules": [
      {
        "foreground": "64748b",
        "token": "comment"
      },
      {
        "foreground": "059669",
        "token": "constant"
      },
      {
        "foreground": "10b981",
        "token": "entity"
      },
      {
        "foreground": "047857",
        "token": "keyword"
      },
      {
        "foreground": "047857",
        "token": "storage"
      },
      {
        "foreground": "059669",
        "token": "string"
      },
      {
        "foreground": "059669",
        "token": "meta.verbatim"
      },
      {
        "foreground": "059669",
        "token": "support"
      },
      {
        "foreground": "dc2626",
        "fontStyle": "italic",
        "token": "invalid.deprecated"
      },
      {
        "foreground": "0f172a",
        "background": "ef4444",
        "token": "invalid.illegal"
      },
      {
        "foreground": "10b981",
        "fontStyle": "italic",
        "token": "entity.other.inherited-class"
      },
      {
        "foreground": "10b981",
        "token": "string constant.other.placeholder"
      },
      {
        "foreground": "0f172a",
        "token": "meta.function-call.py"
      },
      {
        "foreground": "64748b",
        "token": "meta.tag"
      },
      {
        "foreground": "64748b",
        "token": "meta.tag entity"
      },
      {
        "foreground": "0f172a",
        "token": "entity.name.section"
      }
    ],
    "colors": {
      "editor.foreground": "#0f172a",
      "editor.background": "#ecfdf5",
      "editor.selectionBackground": "#a7f3d0",
      "editor.lineHighlightBackground": "#d1fae5",
      "editorCursor.foreground": "#10b981",
      "editorWhitespace.foreground": "#6ee7b7"
    }
  },

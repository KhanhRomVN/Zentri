{
  "name": "ForestLight",
  "monaco": {
    "base": "vs",
    "inherit": true,
    "rules": [
      {
        "foreground": "64748b",
        "token": "comment"
      },
      {
        "foreground": "16a34a",
        "token": "constant"
      },
      {
        "foreground": "22c55e",
        "token": "entity"
      },
      {
        "foreground": "15803d",
        "token": "keyword"
      },
      {
        "foreground": "15803d",
        "token": "storage"
      },
      {
        "foreground": "059669",
        "token": "string"
      },
      {
        "foreground": "059669",
        "token": "meta.verbatim"
      },
      {
        "foreground": "16a34a",
        "token": "support"
      },
      {
        "foreground": "dc2626",
        "fontStyle": "italic",
        "token": "invalid.deprecated"
      },
      {
        "foreground": "0f172a",
        "background": "ef4444",
        "token": "invalid.illegal"
      },
      {
        "foreground": "22c55e",
        "fontStyle": "italic",
        "token": "entity.other.inherited-class"
      },
      {
        "foreground": "22c55e",
        "token": "string constant.other.placeholder"
      },
      {
        "foreground": "0f172a",
        "token": "meta.function-call.py"
      },
      {
        "foreground": "64748b",
        "token": "meta.tag"
      },
      {
        "foreground": "64748b",
        "token": "meta.tag entity"
      },
      {
        "foreground": "0f172a",
        "token": "entity.name.section"
      }
    ],
    "colors": {
      "editor.foreground": "#0f172a",
      "editor.background": "#f0fdf4",
      "editor.selectionBackground": "#dcfce7",
      "editor.lineHighlightBackground": "#f7fee7",
      "editorCursor.foreground": "#22c55e",
      "editorWhitespace.foreground": "#bbf7d0"
    }
  },
  "tailwind": {
    "primary": "34 197 94",
    "background": "240 253 244",

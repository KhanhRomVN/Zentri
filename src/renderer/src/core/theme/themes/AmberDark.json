{
  "name": "AmberDark",
  "monaco": {
    "base": "vs-dark",
    "inherit": true,
    "rules": [
      {
        "foreground": "94a3b8",
        "token": "comment"
      },
      {
        "foreground": "fbbf24",
        "token": "constant"
      },
      {
        "foreground": "fcd34d",
        "token": "entity"
      },
      {
        "foreground": "f59e0b",
        "token": "keyword"
      },
      {
        "foreground": "f59e0b",
        "token": "storage"
      },
      {
        "foreground": "34d399",
        "token": "string"
      },
      {
        "foreground": "34d399",
        "token": "meta.verbatim"
      },
      {
        "foreground": "fbbf24",
        "token": "support"
      },
      {
        "foreground": "f87171",
        "fontStyle": "italic",
        "token": "invalid.deprecated"
      },
      {
        "foreground": "fffbeb",
        "background": "dc2626",
        "token": "invalid.illegal"
      },
      {
        "foreground": "fcd34d",
        "fontStyle": "italic",
        "token": "entity.other.inherited-class"
      },
      {
        "foreground": "fcd34d",
        "token": "string constant.other.placeholder"
      },
      {
        "foreground": "fffbeb",
        "token": "meta.function-call.py"
      },
      {
        "foreground": "94a3b8",
        "token": "meta.tag"
      },
      {
        "foreground": "94a3b8",
        "token": "meta.tag entity"
      },
      {
        "foreground": "fffbeb",
        "token": "entity.name.section"
      }
    ],
    "colors": {
      "editor.foreground": "#fffbeb",
      "editor.background": "#1f1a0a",
      "editor.selectionBackground": "#451a03",
      "editor.lineHighlightBackground": "#2d1f0f",
      "editorCursor.foreground": "#f59e0b",
      "editorWhitespace.foreground": "#fbbf24"
    }
  },
